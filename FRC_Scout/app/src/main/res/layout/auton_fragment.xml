<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/backgroundColor"
            android:orientation="vertical"
            android:paddingLeft="15dp"
            android:paddingTop="20dp"
            android:paddingRight="15dp">

            <LinearLayout
                android:layout_width="500dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="2dp"
                android:paddingBottom="15dp">

                <TextView
                    android:id="@+id/leave_title"
                    style="?android:listSeparatorTextViewStyle"
                    android:layout_width="190dp"
                    android:layout_height="wrap_content"
                    android:text="Leave"
                    android:textColor="@color/textPrimary"
                    android:textSize="24sp" />

                <CheckBox
                    android:id="@+id/leave_checkbox"
                    android:layout_width="262dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="16dp"
                    android:buttonTint="@color/textPrimary"
                    android:text="Leave Starting Zone"
                    android:textColor="@color/textPrimary"
                    android:textSize="22sp" />

            </LinearLayout>

            <TextView
                android:id="@+id/amp_scoring_title"
                style="?android:listSeparatorTextViewStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/top_rounded_rect"
                android:paddingTop="20dp"
                android:paddingBottom="1dp"
                android:text="CORAL SCORING"
                android:textColor="@color/specialTextPrimary"
                android:textSize="24sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/autonUpperCardColor"
            android:orientation="horizontal"
            android:paddingBottom="1dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/autonUpperCardColor"
                android:orientation="horizontal"
                android:paddingBottom="1dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:orientation="horizontal"
                    android:layout_marginTop="4dp"
                    android:layout_marginStart="16dp"
                    android:layout_weight="4"
                    android:paddingBottom="1dp">

                    <TextView
                        android:id="@+id/auton_coral_scoring_L1"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="L1:"
                        android:layout_marginTop="8dp"
                        android:layout_marginStart="16dp"
                        android:textSize="22dp"
                        android:textColor="@color/specialTextPrimary"
                        />

                    <TextView
                        android:id="@+id/auton_coral_scoring_text_L1"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="6dp"
                        android:textSize="25sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:orientation="horizontal"
                    android:layout_marginTop="8dp"
                    android:layout_weight="4"
                    android:paddingBottom="1dp">

                    <TextView
                        android:id="@+id/auton_coral_scoring_L2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="4dp"
                        android:text="L2:"
                        android:layout_marginStart="16dp"
                        android:textSize="22dp"
                        android:textColor="@color/specialTextPrimary"
                        />

                    <TextView
                        android:layout_marginTop="2dp"
                        android:id="@+id/auton_coral_scoring_text_L2"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:textSize="25sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:orientation="horizontal"
                    android:layout_marginTop="8dp"
                    android:layout_weight="4"
                    android:paddingBottom="1dp">

                    <TextView
                        android:id="@+id/auton_coral_scoring_L3"
                        android:layout_marginTop="4dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="L3:"
                        android:layout_marginStart="16dp"
                        android:textSize="22dp"
                        android:textColor="@color/specialTextPrimary"
                        />

                    <TextView
                        android:layout_marginTop="2dp"
                        android:id="@+id/auton_coral_scoring_text_L3"
                        android:layout_width="100dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:textSize="25sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="8dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:orientation="horizontal"
                    android:layout_marginStart="16dp"
                    android:layout_weight="4"
                    android:paddingBottom="1dp">

                    <TextView
                        android:id="@+id/auton_coral_scoring_L4"
                        android:layout_marginTop="4dp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="L4:"
                        android:layout_marginStart="16dp"
                        android:textSize="22dp"
                        android:textColor="@color/specialTextPrimary"
                        />

                    <TextView
                        android:id="@+id/auton_coral_scoring_text_L4"
                        android:layout_width="100dp"
                        android:layout_marginTop="2dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:textSize="25sp" />

                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/autonUpperCardColor"
            android:orientation="horizontal"
            android:paddingBottom="1dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/autonUpperCardColor"
                android:orientation="vertical"
                android:paddingBottom="10dp">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:orientation="horizontal"
                    android:layout_marginStart="80dp"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="1dp">

                    <Button
                        android:id="@+id/auton_coral_scoring_decr"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#9F3FB5"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_text"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/auton_coral_scoring_decr"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/increase"
                        android:layout_toRightOf="@+id/auton_coral_scoring_text"
                        android:textColor="#9F3FB5"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_decr_disabled"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#696969"
                        android:backgroundTint="#BEBEBE"
                        android:layout_alignRight="@+id/auton_coral_scoring_decr"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_text_disabled"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr_disabled"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:backgroundTint="#BEBEBE"
                        android:text="@string/increase"
                        android:textColor="#696969"
                        android:layout_alignLeft="@+id/auton_coral_scoring_incr"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_decr_L1"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#FFA9DD"
                        android:backgroundTint="#A9FFCB"
                        android:layout_alignRight="@+id/auton_coral_scoring_decr"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_button_text_L1"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr_L1"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:backgroundTint="#A9FFCB"
                        android:text="@string/increase"
                        android:textColor="#FFA9DD"
                        android:layout_alignLeft="@+id/auton_coral_scoring_incr"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_decr_L2"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#EA6E5E"
                        android:backgroundTint="#5EDAEA"
                        android:layout_alignRight="@+id/auton_coral_scoring_decr"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_button_text_L2"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr_L2"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:backgroundTint="#5EDAEA"
                        android:text="@string/increase"
                        android:textColor="#EA6E5E"
                        android:layout_alignLeft="@+id/auton_coral_scoring_incr"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_decr_L3"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#A4C837"
                        android:backgroundTint="#5B37C8"
                        android:layout_alignRight="@+id/auton_coral_scoring_decr"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_button_text_L3"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr_L3"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:backgroundTint="#5B37C8"
                        android:text="@string/increase"
                        android:textColor="#A4C837"
                        android:layout_alignLeft="@+id/auton_coral_scoring_incr"
                        android:textSize="30sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_decr_L4"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="16dp"
                        android:background="@drawable/rounded_rect"
                        android:text="@string/decrease"
                        android:textColor="#63E21D"
                        android:backgroundTint="#9C1DE2"
                        android:layout_alignRight="@+id/auton_coral_scoring_decr"
                        android:textSize="30sp" />

                    <TextView
                        android:id="@+id/auton_coral_scoring_button_text_L4"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:textAlignment="center"
                        android:textSize="25sp" />

                    <Button
                        android:id="@+id/auton_coral_scoring_incr_L4"
                        android:layout_width="120dp"
                        android:layout_height="match_parent"
                        android:layout_marginEnd="30dp"
                        android:background="@drawable/rounded_rect"
                        android:backgroundTint="#9C1DE2"
                        android:text="@string/increase"
                        android:textColor="#63E21D"
                        android:layout_alignLeft="@+id/auton_coral_scoring_incr"
                        android:textSize="30sp" />

                </RelativeLayout>

                <RadioGroup
                    android:id="@+id/auton_coral_radio_group"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/amp_notes_title"
                        style="?android:listSeparatorTextViewStyle"
                        android:layout_width="80dp"
                        android:layout_height="match_parent"
                        android:layout_marginStart="8dp"
                        android:background="@color/autonUpperCardColor"
                        android:text="LEVEL:"
                        android:textColor="@color/specialTextSecondary"
                        android:textSize="20sp" />

                    <RadioButton
                        android:layout_marginTop="5dp"
                        android:id="@+id/auton_coral_L1"
                        android:layout_marginStart="16dp"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/specialTextPrimary"
                        android:text="1"
                        android:textColor="@color/specialTextPrimary"
                        android:textSize="20sp" />

                    <RadioButton
                        android:layout_marginTop="5dp"
                        android:id="@+id/auton_coral_L2"
                        android:layout_marginStart="16dp"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/specialTextPrimary"
                        android:text="2"
                        android:textColor="@color/specialTextPrimary"
                        android:textSize="20sp" />

                    <RadioButton
                        android:layout_marginTop="5dp"
                        android:id="@+id/auton_coral_L3"
                        android:layout_marginStart="16dp"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/specialTextPrimary"
                        android:text="3"
                        android:textColor="@color/specialTextPrimary"
                        android:textSize="20sp" />

                    <RadioButton
                        android:layout_marginTop="5dp"
                        android:id="@+id/auton_coral_L4"
                        android:layout_marginStart="16dp"
                        android:layout_width="70dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/specialTextPrimary"
                        android:text="4"
                        android:textColor="@color/specialTextPrimary"
                        android:textSize="20sp" />

                    <RadioButton
                        android:layout_marginTop="5dp"
                        android:id="@+id/auton_coral_none"
                        android:layout_marginStart="16dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:buttonTint="@color/specialTextPrimary"
                        android:text="None"
                        android:textColor="@color/specialTextPrimary"
                        android:textSize="20sp" />

                </RadioGroup>

            </LinearLayout>
        </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/autonUpperCardColor"
                android:orientation="horizontal"
                android:paddingTop="2dp"
                android:paddingBottom="1dp">

            </LinearLayout>

            <TextView
                android:id="@+id/auton_speaker_scoring_title"
                style="?android:listSeparatorTextViewStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EFE2FF"
                android:paddingTop="20dp"
                android:text="Algae SCORING"
                android:textColor="@color/specialTextPrimary"
                android:textSize="24sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/autonUpperCardColor"
                android:orientation="horizontal"
                android:paddingBottom="1dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/autonUpperCardColor"
                    android:backgroundTint="#EFE2FF"
                    android:orientation="horizontal"
                    android:paddingBottom="1dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#EFE2FF"
                        android:orientation="horizontal"
                        android:layout_marginTop="4dp"
                        android:layout_marginStart="16dp"
                        android:layout_weight="4"
                        android:paddingBottom="1dp">

                        <TextView
                            android:id="@+id/auton_algae_net"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="Net:"
                            android:layout_marginTop="8dp"
                            android:layout_marginStart="16dp"
                            android:textSize="22dp"
                            android:textColor="@color/specialTextPrimary"
                            />

                        <TextView
                            android:id="@+id/auton_algae_net_scoring"
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="6dp"
                            android:textSize="25sp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#EFE2FF"
                        android:orientation="horizontal"
                        android:layout_marginTop="8dp"
                        android:layout_weight="4"
                        android:paddingBottom="1dp">

                        <TextView
                            android:id="@+id/auton_algae_processor"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="4dp"
                            android:text="Processor:"
                            android:layout_marginStart="16dp"
                            android:textSize="22dp"
                            android:textColor="@color/specialTextPrimary"
                            />

                        <TextView
                            android:layout_marginTop="2dp"
                            android:id="@+id/auton_algae_processor_scoring"
                            android:layout_width="100dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:textSize="25sp" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#EFE2FF"
                android:orientation="horizontal"
                android:paddingBottom="1dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#EFE2FF"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#EFE2FF"
                        android:orientation="horizontal"
                        android:layout_marginStart="80dp"
                        android:layout_marginTop="10dp">

                        <Button
                            android:id="@+id/auton_algae_scoring_decr"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:background="@drawable/rounded_rect"
                            android:text="@string/decrease"
                            android:textColor="#9F3FB5"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/auton_algae_scoring_text"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:layout_toRightOf="@+id/auton_algae_scoring_decr"
                            android:textSize="25sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_incr"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/rounded_rect"
                            android:text="@string/increase"
                            android:layout_toRightOf="@+id/auton_algae_scoring_text"
                            android:textColor="#9F3FB5"
                            android:textSize="30sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_decr_disabled"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:background="@drawable/rounded_rect"
                            android:text="@string/decrease"
                            android:textColor="#696969"
                            android:backgroundTint="#BEBEBE"
                            android:layout_alignRight="@+id/auton_algae_scoring_decr"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/auton_algae_scoring_text_disabled"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="25sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_incr_disabled"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/rounded_rect"
                            android:backgroundTint="#BEBEBE"
                            android:text="@string/increase"
                            android:textColor="#696969"
                            android:layout_alignLeft="@+id/auton_algae_scoring_incr"
                            android:textSize="30sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_decr_net"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:background="@drawable/rounded_rect"
                            android:text="@string/decrease"
                            android:textColor="#FFA9DD"
                            android:backgroundTint="#A9FFCB"
                            android:layout_alignRight="@+id/auton_algae_scoring_decr"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/auton_algae_scoring_button_text_net"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="25sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_incr_net"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/rounded_rect"
                            android:backgroundTint="#A9FFCB"
                            android:text="@string/increase"
                            android:textColor="#FFA9DD"
                            android:layout_alignLeft="@+id/auton_algae_scoring_incr"
                            android:textSize="30sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_decr_processor"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="16dp"
                            android:background="@drawable/rounded_rect"
                            android:text="@string/decrease"
                            android:textColor="#EA6E5E"
                            android:backgroundTint="#5EDAEA"
                            android:layout_alignRight="@+id/auton_algae_scoring_decr"
                            android:textSize="30sp" />

                        <TextView
                            android:id="@+id/auton_algae_scoring_button_text_processor"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="25sp" />

                        <Button
                            android:id="@+id/auton_algae_scoring_incr_processor"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:layout_marginEnd="30dp"
                            android:background="@drawable/rounded_rect"
                            android:backgroundTint="#5EDAEA"
                            android:text="@string/increase"
                            android:textColor="#EA6E5E"
                            android:layout_alignLeft="@+id/auton_algae_scoring_incr"
                            android:textSize="30sp" />

                    </RelativeLayout>

                    <RadioGroup
                        android:id="@+id/auton_algae_radio_group"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <RadioButton
                            android:layout_marginTop="5dp"
                            android:id="@+id/auton_algae_net_button"
                            android:layout_marginStart="40dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:buttonTint="@color/specialTextPrimary"
                            android:text="Net"
                            android:textColor="@color/specialTextPrimary"
                            android:textSize="20sp" />

                        <RadioButton
                            android:layout_marginTop="5dp"
                            android:id="@+id/auton_algae_processor_button"
                            android:layout_marginStart="16dp"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:buttonTint="@color/specialTextPrimary"
                            android:text="Processor"
                            android:textColor="@color/specialTextPrimary"
                            android:textSize="20sp" />

                        <RadioButton
                            android:layout_marginTop="5dp"
                            android:id="@+id/auton_algae_none_button"
                            android:layout_marginStart="16dp"
                            android:layout_width="70dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:buttonTint="@color/specialTextPrimary"
                            android:text="None"
                            android:textColor="@color/specialTextPrimary"
                            android:textSize="20sp" />

                    </RadioGroup>

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/bottom_rounded_rect"
                android:backgroundTint="#EFE2FF"
                android:orientation="horizontal"
                android:paddingBottom="10dp">

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>
