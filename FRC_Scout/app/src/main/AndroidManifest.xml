<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frc2135.android.frc_scout">

    <uses-permission android:name="aandroid.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.frc2135.android.frc_scout.MatchListActivity">
        </activity>

        <activity
            android:name="com.frc2135.android.frc_scout.PreMatchActivity"
            android:label="Input Match Data"></activity>

        <activity
            android:name="com.frc2135.android.frc_scout.ScoutingActivity"
            android:label="Input Match Data"></activity>

        <activity
            android:name="com.frc2135.android.frc_scout.Splash"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="FRC Scout 2022">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </application>

</manifest>
