<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/scout_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/scout_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >

        <activity
            android:name="com.frc2135.android.frc_scout.MatchListActivity"
            android:exported="true"
            />

        <activity
            android:name="com.frc2135.android.frc_scout.PreMatchActivity"
            android:exported="true"
            android:label="Input Match Data"
            />

        <activity
            android:name="com.frc2135.android.frc_scout.ScoutingActivity"
            android:exported="true"
            android:label="Input Match Data"
            />

        <activity
            android:name="com.frc2135.android.frc_scout.Splash"
            android:exported="true"
            android:label="FRC Scout 2023"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
